{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:custom_model_data",
    "cases": [
        {
            "when": "lvl1_key",
            "model": {
              "type": "minecraft:model",
              "model": "next:crate/lvl1_key"
            }
        },
        {
            "when": "lvl2_key",
            "model": {
              "type": "minecraft:model",
              "model": "next:crate/lvl2_key"
            }
        },
        {
            "when": "lvl3_key",
            "model": {
              "type": "minecraft:model",
              "model": "next:crate/lvl3_key"
            }
        },
        {
            "when": "lvl4_key",
            "model": {
              "type": "minecraft:model",
              "model": "next:crate/lvl4_key"
            }
        }
    ],
    "fallback": {
      "type": "minecraft:model",
      "model": "minecraft:item/tripwire_hook"
    }
  }
}